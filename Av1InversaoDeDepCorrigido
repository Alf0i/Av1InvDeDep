using system;

public class Contador{
    int horas;
    public AlarmeInterface _al; 

    public Contador contador(int hora){
        this.horas = hora;
    }

    fun VerificaHoras(){
        if(horas == 17){
            return(_al.AcionarSom, _al.Vibrar);
        }
    }

}

public class Alarme: AlarmeInterface{
    override fun Vibrar(){
        Console.WriteLine($" Vibrando!!!");
    }

    override fun AcionarSom(){
        Console.WriteLine($" trim-trim-trim...");
    }

}

public interface AlarmeInterface{
    public fun Vibrar(){}

    public fun AcionarSom(){}

}
